div.content-wrapper
	div.breadcrumb.clearfix
		ul
			li
				a(href='/')
					i.fa.fa-home.fa-2x
			li.active Add Readings...
	div#powerwidgets.row
		div.col-md-12.bootstrap-grid
			#point-image.powerwidget.powerwidget-as-portlet.green.powerwidget-sortable(data-widget-editbutton='false',role='widget')
				header
					h2 Map
				div.inner-spacer
					form#input-masking.orb-form(class='{{addForm}}')
						fieldset
							.row(style='padding:20px;')
								.col-md-4.col-sm-4.col-xs-4(style='margin:0;padding:0;')
									section
										label.label Projects
										label.select.select-multiple
											select(multiple='',ng-model='form.projectsSelect')
												option(value='Water Quality in Island') Water Quality in Island
												option(value='Oilfields') Oilfields
												option(value='Biodiversity Checking') Biodiversity Checking
												option(value='Forest Inventory') Forest Inventory
												option(value='Biomass Mapping') Biomass Mapping
												option(value='Forest Species') Forest Species
												option(value='North River Basin') North River Basin
												option(value='Inventory on South') Inventory on South
												option(value='Paper Project') Paper Project
								.col-md-4.col-sm-4.col-xs-4(style='margin:0;padding:0;')
									section
										label.label Indicators
										label.select.select-multiple
											select(multiple='',ng-model='form.indicatorsSelect')
												option(value='1') Water Quality
												option(value='2') Budget
												option(value='3') Biodiversity Checking
												option(value='4') Forest Inventory
												option(value='5') Biomass Mapping
								.col-md-4.col-sm-4.col-xs-4(style='margin:0;padding:0;')
									section
										label.label Parameters
										label.select.select-multiple
											select(multiple='',ng-model='form.parametersSelect')
												option(value='1') pH
												option(value='2') Coliphorms
												option(value='3') Biodiversity
												option(value='4') Particles
												option(value='5') Metal
							.row
								.col-md-11.col-sm-11.col-xs-11
									section
										label.label Single Value
										label.input
											i.icon-append.fa.fa-tachometer
											input#phone(placeholder='Input a single value...', name='value', ng-model='form.value')
									section
										label.label Multiple Values
										label.input
											//- i.icon-append.fa.fa-crosshairs
											textarea(placeholder='... or paste values from Excel', ng-model='sentence',rows='10',style='width:100%;')
							
									button.btn.btn-default(ng-click='submitNewReading()') Add Reading
									p(style='color:green;margin-top:15px;') {{lastAddedMsg}}
					p: a(href='/projects') Back


